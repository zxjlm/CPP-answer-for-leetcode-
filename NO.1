class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
    map<int, int> numsMap;
    for (auto i = 0; i < nums.size(); i++)
        numsMap[nums[i]] = i;
    for (auto j = 0; j < nums.size(); j++)
    {
        int v = target - nums[j];
        if (numsMap.count(v) && numsMap[v] != j)
        {
            return vector<int> {numsMap[v],j};
        }
    }
    return vector<int> {0,0};
}
};
